{
  "name": "kovah.de-blog",
  "version": "2.0.0",
  "description": "Kovah.de Blog source",
  "author": "Kevin Woblick <mail@kovah.de>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "serve": "eleventy --serve",
    "serve-dev": "DEBUG=Eleventy* eleventy --serve",
    "build": "npm run production && ELEVENTY_PRODUCTION=true eleventy"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.8.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.2",
    "@tailwindcss/typography": "^0.4.0",
    "date-fns": "^2.21.3",
    "html-minifier": "^4.0.0",
    "laravel-mix": "^6.0.27",
    "luxon": "^1.27.0",
    "postcss": "^8.3.6",
    "tailwindcss": "^2.2.7"
  }
}
